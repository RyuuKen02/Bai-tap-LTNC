#include <iostream>
#include <algorithm>
using namespace std;

void update(int&a, int&b)
{
    int tong = a+b;
    int hieu = abs(a-b);
    a = tong;
    b = hieu;
}

int main() {
    int a,b;
    cin>>a>>b;
    update(a,b);
    cout<<a<<endl<<b;
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
